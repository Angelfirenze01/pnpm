{
  "name": "@pnpm/nm-linker",
  "version": "0.0.1",
  "description": "Installs dependencies into npm-style node_modules directories",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "_test": "jest",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/nm-linker",
  "keywords": [
    "pnpm6",
    "pnpm",
    "linker"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/nm-linker#readme",
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "workspace:7.4.0",
    "@types/normalize-path": "^3.0.0",
    "@types/ramda": "0.27.39",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@pnpm/config": "workspace:13.2.0",
    "dependency-path": "workspace:8.0.4",
    "normalize-path": "^3.0.0",
    "ramda": "^0.27.1"
  },
  "funding": "https://opencollective.com/pnpm"
}
